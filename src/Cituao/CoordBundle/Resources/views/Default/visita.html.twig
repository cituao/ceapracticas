{% extends 'CituaoCoordBundle::layout.html.twig' %}


{% block js  %}
<script>
$( "#practicantes" ).css("background-color","#000");
$( "#practicantes img" ).css("opacity","0.4");
$( "#practicantes h3" ).css("opacity","0.4");
</script>

{% endblock %}

{% block content  %}
{{ parent() }}
<div class="content">
{% block upSection  %}
<div class = "upSection">
	
	<h1 class="sectionTitle">Consultando la visita de presentaci√≥n</h1>

	<a href="javascript:history.back()" class = "button"><img src="{{ asset('img/regresar.png') }}"/><h4>Regresar</h4></a>
</div>
{% endblock %}
{% block downSection  %}
<div class = "downSection">
	<div class="inform">
		<form action="" method="post">
			<div class="activity">	
				<div class="inputContainer">
					<h2>Comentario de la actividad:</h2>
					<div class="input nomodificable">
						<textarea cols="125" rows="10" readonly>{{ datos.comentario }}</textarea>
					</div>
				</div>
			</div>

		</form>	

	</div><!-- inform -->
</div><!-- downSection -->

{% endblock %}
</div><!-- content -->
{% endblock %}
