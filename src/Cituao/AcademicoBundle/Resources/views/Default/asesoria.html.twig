{% extends 'CituaoAcademicoBundle::layout.html.twig' %}

{% block programa %}
{{ programa.nombre }}
{% endblock %}

{% block js  %}
<script>
$( "#practicantes" ).css("background-color","#000");
$( "#practicantes img" ).css("opacity","0.4");
$( "#practicantes h3" ).css("opacity","0.4");
</script>

{% endblock %}

{% block content  %}
	<div class="content">
		<h2 id="titlee">Consultando la Asesor√≠a # {{ datos.numase }}</h2>

		<div id = "menuop">
			<ul>
			<li><a href="javascript:history.back()" class = "baropcion">Regresar</a></li>
			</ul>
		</div>

	
	<form action="" method="post">
		<div>	
		<label>Asesor:</label>
		<textarea cols="125" rows="10" readonly>{{ datos.docase }}</textarea>
		</div>
		
		<div>	
		<label>Practicante:</label>
		<textarea cols="125" rows="10" readonly>{{ datos.docpra }}</textarea>
		</div>

	</form>	
	</div>
{% endblock %}
